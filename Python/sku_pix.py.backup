#!/usr/bin/env python

import os

sku_input = raw_input("SKU: ")
folder = '/images/' + raw_input("Folder: ")
pix_list = []

filelist = os.listdir(folder)

for i in filelist:
	sku_output = i.split('_')[0]
	#sku_ext = i.split('_')[-1]
	if sku_output == sku_input:
		pix_list.append(i)

for position, f in enumerate(pix_list):
	if f[-8:] == 'full.jpg':
		if position != 0:
			del pix_list[position]
			pix_list.insert(0, f)
	elif f[-7:] == '250.jpg' or f[-7:] == '300.jpg':
		if position != 0:
			del pix_list[position]
			pix_list.insert(0, f)

print pix_list